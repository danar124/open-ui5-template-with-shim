{
    "env": {
        "browser": true
    },
    "globals": {
        "sap": true,
        "jQuery": true,
        "$": true
    },
    "rules": {
        "curly":"error",
        "semi": 2,
        "comma-dangle": 2,
        "no-alert": 2,
        "no-console": 2,
        "no-debugger": 2,
        "key-spacing": 2,
        "no-shadow": 2,
        "no-undef": 2,
        "block-scoped-var": 1,
        "brace-style": [2, "1tbs", { "allowSingleLine": true }],
        "consistent-this": [1, "that", "s", "target", "t"],
        "no-div-regex": 2,
        "no-floating-decimal": 2,
        "no-self-compare": 2,
        "no-nested-ternary": 2,
        "no-unused-vars": [2, {"vars":"all", "args":"none"}],
        "radix": 1,
        "keyword-spacing": 2,
        "space-unary-ops": [
            2, {
            "words": true
            }
        ],
        "wrap-iife": [2, "any"],
 
        "camelcase": 1,
        "consistent-return": 1,
        "max-nested-callbacks": [1, 3],
        "new-cap": 0,
        "no-extra-boolean-cast": 2,
        "no-lonely-if": 1,
        "no-new": 1,
        "no-new-wrappers": 1,
        "no-redeclare": 1,
        "no-unused-expressions": 1,
        "no-use-before-define": [2, "nofunc"],
        "no-warning-comments": 1,
        "strict": 2,
        "valid-jsdoc": [0, {
            "requireReturn": false
        }],
        "default-case": 1,
        "dot-notation": 0,
        "eol-last": 0,
        "eqeqeq": 2,
        "no-underscore-dangle": 0,
        "quotes": 0
    }
}
